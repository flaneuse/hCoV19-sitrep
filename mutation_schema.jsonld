      ]
    },
    {
      "@id": "outbreakmutations:gisaid_epi_isl",
      "@type": "rdf:Property",
      "rdfs:comment": "GISAID ID for the virus sequence",
      "rdfs:label": "gisaid_epi_isl",
      "schema:domainIncludes": {
        "@id": "outbreakmutations:RnaSequence"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
